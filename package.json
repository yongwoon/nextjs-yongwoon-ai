  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 refine dev",
    "build": "refine build",
    "start": "refine start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && eslint --fix .",
    "refine": "refine",
    "docker:dev": "docker compose up --build",
    "docker:down": "docker compose down",
    "docker:clean": "docker compose down -v --remove-orphans",
    "db:migrate": "echo 'Run the SQL schema in docs/setup/database-schema.sql in your Supabase dashboard'",
    "db:seed": "echo 'Seed data is included in the schema file'",
    "env:check": "node -e \"console.log('Environment check - implement validation script')\"",
    "ai:test": "echo 'Test AI connections - implement test script'",
    "cache:clear": "echo 'Clear Redis cache - implement cache clearing script'",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
